{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "line-talk-rag",
  "main": "src/index.ts",
  "compatibility_date": "2025-06-14",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "ai": {
    "binding": "AI"
  },
  "vectorize": [
    {
      "binding": "VECTORIZE",
      "index_name": "line-talk-rag-index"
    }
  ],
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  }
}
